    <!-- REGLE 1: Detection de l'echec -->
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <rdfs:label>Regle Echec</rdfs:label>
        <rdfs:comment>Un roi est en echec si une piece adverse peut atteindre sa case</rdfs:comment>
        <swrl:body rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Roi"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#roi"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#estSurCase"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#roi"/>
                <swrl:argument2 rdf:resource="urn:swrl:var#caseRoi"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Piece"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#pieceAdverse"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#peutAtteindreCase"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#pieceAdverse"/>
                <swrl:argument2 rdf:resource="urn:swrl:var#caseRoi"/>
            </rdf:Description>
        </swrl:body>
        <swrl:head rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Echec"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#etat"/>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>

    <!-- REGLE 2: Detection de capture -->
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <rdfs:label>Regle Capture</rdfs:label>
        <rdfs:comment>Si un coup deplace une piece vers une case occupee par une piece adverse, c'est une capture</rdfs:comment>
        <swrl:body rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Coup"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#coup"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#caseArrivee"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#coup"/>
                <swrl:argument2 rdf:resource="urn:swrl:var#caseDest"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#contientPiece"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#caseDest"/>
                <swrl:argument2 rdf:resource="urn:swrl:var#pieceCapturee"/>
            </rdf:Description>
        </swrl:body>
        <swrl:head rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Capture"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#coup"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#capturePiece"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#coup"/>
                <swrl:argument2 rdf:resource="urn:swrl:var#pieceCapturee"/>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>

    <!-- REGLE 3: Promotion du pion blanc -->
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <rdfs:label>Regle Promotion Pion Blanc</rdfs:label>
        <rdfs:comment>Un pion blanc qui atteint la rangee 8 doit etre promu</rdfs:comment>
        <swrl:body rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Pion"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#pion"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#couleur"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#pion"/>
                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">blanc</swrl:argument2>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#estSurCase"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#pion"/>
                <swrl:argument2 rdf:resource="urn:swrl:var#case"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#rangee"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#case"/>
                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</swrl:argument2>
            </rdf:Description>
        </swrl:body>
        <swrl:head rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Promotion"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#coup"/>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>

    <!-- REGLE 4: Promotion du pion noir -->
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <rdfs:label>Regle Promotion Pion Noir</rdfs:label>
        <rdfs:comment>Un pion noir qui atteint la rangee 1 doit etre promu</rdfs:comment>
        <swrl:body rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Pion"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#pion"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#couleur"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#pion"/>
                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">noir</swrl:argument2>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#estSurCase"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#pion"/>
                <swrl:argument2 rdf:resource="urn:swrl:var#case"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#rangee"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#case"/>
                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</swrl:argument2>
            </rdf:Description>
        </swrl:body>
        <swrl:head rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Promotion"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#coup"/>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>

    <!-- REGLE 5: Validation du roque -->
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <rdfs:label>Regle Roque Valide</rdfs:label>
        <rdfs:comment>Le roque est valide si le roi et la tour n'ont jamais bouge</rdfs:comment>
        <swrl:body rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Roque"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#coup"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Roi"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#roi"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#TourPiece"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#tour"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#aBouge"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#roi"/>
                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</swrl:argument2>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#aBouge"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#tour"/>
                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">false</swrl:argument2>
            </rdf:Description>
        </swrl:body>
        <swrl:head rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#estValide"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#coup"/>
                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrl:argument2>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>

    <!-- REGLE 6: Marquer une piece comme ayant bouge -->
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <rdfs:label>Regle Piece a Bouge</rdfs:label>
        <rdfs:comment>Apres qu'une piece a ete deplacee par un coup, elle est marquee comme ayant bouge</rdfs:comment>
        <swrl:body rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Coup"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#coup"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#deplacePiece"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#coup"/>
                <swrl:argument2 rdf:resource="urn:swrl:var#piece"/>
            </rdf:Description>
        </swrl:body>
        <swrl:head rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#aBouge"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#piece"/>
                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrl:argument2>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>

    <!-- REGLE 7: Piece capturee -->
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <rdfs:label>Regle Piece Capturee</rdfs:label>
        <rdfs:comment>Une piece capturee est marquee comme capturee</rdfs:comment>
        <swrl:body rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Coup"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#coup"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#capturePiece"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#coup"/>
                <swrl:argument2 rdf:resource="urn:swrl:var#piece"/>
            </rdf:Description>
        </swrl:body>
        <swrl:head rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#estCapturee"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#piece"/>
                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrl:argument2>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>

    <!-- REGLE 8: Menace entre pieces -->
    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <rdfs:label>Regle Menace</rdfs:label>
        <rdfs:comment>Une piece menace une autre si elle peut atteindre sa case</rdfs:comment>
        <swrl:body rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Piece"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#piece1"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                <swrl:classPredicate rdf:resource="http://www.exemple.org/chess#Piece"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#piece2"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#estSurCase"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#piece2"/>
                <swrl:argument2 rdf:resource="urn:swrl:var#case"/>
            </rdf:Description>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#peutAtteindreCase"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#piece1"/>
                <swrl:argument2 rdf:resource="urn:swrl:var#case"/>
            </rdf:Description>
        </swrl:body>
        <swrl:head rdf:parseType="Collection">
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                <swrl:propertyPredicate rdf:resource="http://www.exemple.org/chess#menacePiece"/>
                <swrl:argument1 rdf:resource="urn:swrl:var#piece1"/>
                <swrl:argument2 rdf:resource="urn:swrl:var#piece2"/>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>

</rdf:RDF>